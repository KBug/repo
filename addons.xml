<?xml version="1.0" encoding="utf-8"?>
<addons>
  <!-- Australian Catchup TV Plugins -->
  <addon id="plugin.video.catchuptv.au.sbs" name="SBS CatchUp TV"
  version="0.1.1" provider-name="adammw111">
    <requires>
      <import addon="xbmc.python" version="1.0" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
      <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
      <platform>all</platform>
      <minversion>31542</minversion>
      <summary>SBS CatchUp TV Video Player</summary>
      <description>Watch latest episodes, sneak peeks, exclusive
      internet extras and other videos from SBS
      Online.</description>
      <disclaimer>You must be within Australia to use watch these
      videos.[CR]"SBS", SBS Logos and other works are copyright of
      their respective owners.</disclaimer>
    </extension>
  </addon>
