      <checksum>https://raw.github.com/xbmc-catchuptv-au/repo/master/addons.xml.md5</checksum>
      <datadir zip="true">https://github.com/xbmc-catchuptv-au/repo/raw/master/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
